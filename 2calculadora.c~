#include <stdio.h>
/**

@fn Librería principal                                                                                                 
   @param 
*/


void mostrarInstrucciones (void);
int ingresarEdad(void);
void RestauranteBar(void);
/**

@fn Procedimiento que muestra las instrucciones del programa.                                                                                                                  
   @param 
*/


 int main (void)
{
mostrarInstrucciones();

 int edad;
 edad = ingresarEdad();
 RestauranteBar();
/**                                                                                                                                                                                                         
   @in Función principal                                                                                                                                                                                    
   @return Integer:0, indicar que el programa se ejecuto de manera correcta                                                                                                                                 
                                                                                                                                                                                                            
*/

  return 0;
}


void mostrarInstrucciones (void){

  printf("\n\n\n\nPrograma que hace uso de if else\n\n");
  printf("Restaurante bar elaborado por: ");
  printf("Natalia Velázquez Rosas\n\n\n\n\n");
 return;

}


/**

@fn Procedimiento que pide la edad                                                                                                               
   @param 
*/


 
int ingresarEdad (void){


  int edad =0;

  printf("\n\nIngresa tu edad:  ");
   scanf (" %d", &edad);

   if(edad <0){
     printf("\nIngresa una edad valida:  ");
     }

   else if (edad>150){
 printf("\nIngresa una edad valida:  ");

   }
   
   return edad;
  
}
/**                                                                                               
                                                                                                  
@fn Procedimiento que hace uso de ifs para mostrar el ingreso al restaurante Bar                                                              \
                                                                                                  
   @param                                                                                         
*/





void RestauranteBar (void){

  int edad;
  if (edad < 12){

     printf("\n\nLo sentimos, no puedes entrar )):  ");
  }

  else if (edad >= 12){
    printf("\n\nPuedes ingresar, pero no puede tomar )):  ");

  }
  else{

  printf("\n\nPuedes ingresar y divertirte )):  ");
  
  }



  return;
}

